    "ejs": "^2.5.7",
    "email-validator": "^1.1.1",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.1.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.12",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^4.1.0"
  },
